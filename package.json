{
  "name": "express-starter",
  "version": "1.0.0",
  "description": "A starter template for building Express.js applications",
  "main": "src/server.js",
  "scripts": {
    "test": "jest",
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "model:create": "sequelize model:generate --name",
    "migration:create": "sequelize migration:generate --name",
    "seeder:create": "sequelize seed:generate --name",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo",
    "migrate:undo:all": "sequelize db:migrate:undo:all",
    "migrate:fresh": "npm run migrate:undo:all && npm run migrate",
    "migrate:refresh": "npm run migrate:undo:all && npm run migrate && npm run seed",
    "seed": "sequelize db:seed:all",
    "seed:undo": "sequelize db:seed:undo",
    "seed:undo:all": "sequelize db:seed:undo:all",
    "seed:fresh": "npm run seed:undo:all && npm run seed",
    "db:reset": "npm run migrate:undo:all && npm run migrate && npm run seed",
    "db:setup": "npm run migrate && npm run seed"
  },
  "keywords": [],
  "author": "Bryan Kurnia Akbar",
  "license": "brynnnn12",
  "type": "commonjs",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.5",
    "nodemailer": "^7.0.6",
    "sequelize": "^6.37.7",
    "uuid": "^12.0.0"
  },
  "devDependencies": {
    "jest": "^30.1.3",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.1.4"
  }
}
